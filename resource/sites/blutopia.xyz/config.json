{
    "name": "Blutopia",
    "timezoneOffset": "+0000",
    "schema": "UNIT3D",
    "url": "https://blutopia.xyz/",
    "icon": "https://blutopia.xyz/favicon.ico",
    "tags": ["影视", "综合"],
    "host": "blutopia.xyz",
    "collaborator": ["bimzcy", "lengmianxia", "bright"],
    "selectors": {
        "userBaseInfo": {
            "page": "/",
            "merge": true,
            "fields": {
                "uploaded": {
                    "selector": ["ul.top-nav__ratio-bar i.fa-arrow-up"],
                    "filters": ["query.parent().text().trim().replace(/,/g,'')", "(query)?(query).sizeToNumber():0"]
                },
                "downloaded": {
                    "selector": ["ul.top-nav__ratio-bar i.fa-arrow-down"],
                    "filters": ["query.parent().text().trim().replace(/,/g,'')", "(query)?(query).sizeToNumber():0"]
                },
                "bonus": {
                    "selector": ["ul.top-nav__ratio-bar i.fa-coins"],
                    "filters": ["query.parent().text().trim().replace(/,|\\s/g,'')", "(query)?parseFloat(query):0"]
                },
                "seeding": {
                    "selector": ["ul.top-nav__ratio-bar i.fa-upload"],
                    "filters": ["query.parent().text().trim().replace(/,|\\n/g,'')", "(query)?parseFloat(query):0"]
                }
            }
        },
        "userExtendInfo": {
            "page": "/users/$user.name$",
            "merge": true,
            "fields": {
                "messageCount": {
                    "selector": ["ul.top-nav__icon-bar circle"],
                    "filters": ["query.length?'11':'0'"]
                }
            }
        }
    }
}