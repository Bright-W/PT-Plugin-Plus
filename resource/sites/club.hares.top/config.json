{
   "name":"HaresClub",
   "timezoneOffset":"+0800",
   "description":"2160p/4k 及以上的高清资源站点",
   "url":"https://club.hares.top/",
   "icon":"https://club.hares.top/favicon.ico",
  "tags": ["影视", "纪录片","综合"],
   "schema":"NexusPHP",
   "host":"club.hares.top",
   "collaborator":"kevgao, bright",
  "formerHosts": [],
   "searchEntryConfig":{
      "fieldSelector":{
         "subTitle":{
            "selector":["p.layui-torrents-descr-width"],
            "filters":["query.text()"]
         }
      }
   },
   "selectors":{
      "userExtendInfo":{
         "merge":true,
         "fields":{
            "uploaded": {
               "selector": ["td:contains('上传量') + td"],
               "filters": ["query.text().replace(/,/g,'').match(/([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
            },
            "downloaded": {
               "selector": ["td:contains('下载量') + td"],
               "filters": ["query.text().replace(/,/g,'').match(/([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
            },
            "levelName": {
               "selector": ["td:contains('等级') + td"],
               "filters": ["query.find('b').text()"]
            },
            "bonus":{
               "selector": ["td:contains('奶糖') + td"],
               "filters": ["query.text().replace(/,/g,'')", "parseFloat(query)"]
            },
            "joinTime": {
                "selector": ["td:contains('加入日期') + td"],
                "filters": ["query.text().split(' (')[0]", "dateTime(query).isValid()?dateTime(query).valueOf():query"]
            }
         }
      },
      "userSeedingTorrents": {
         "prerequisites": "!user.seeding",
         "page": "/getusertorrentlistajax.php?userid=$user.id$&type=seeding",
         "fields": {
           "seeding": {
             "selector": ["b:first"],
             "filters": ["query.text()"]
           },
           "seedingSize": {
             "selector": ["tr:not(:eq(0))"],
             "filters": ["jQuery.map(query.find('td:eq(2)'), (item)=>{return $(item).text();})", "_self.getTotalSize(query)"]
           }
         }
      }
   },
  "plugins": [{
    "name": "官方列表",
    "pages": ["/official.php"],
    "scripts": ["/schemas/NexusPHP/common.js", "/schemas/NexusPHP/torrents.js"]
  }],
   "mergeSchemaTagSelectors":true
}