  "selectors": {  
    "userBaseInfo": {
      "merge":true,
      "fields": {
        "messageCount": {
          "selector": [".unread"],
          "filters": ["query.text().match(/(\\d+)/)", "(query && query.length>=2)?parseInt(query[1]):0"]
        }
      }
    },
      "userExtendInfo":{
          "merge":true,
          "fields": { 
            "uploaded": {
               "selector": ["td:contains('上传量') + td"],
               "filters": ["query.text().replace(/,/g,'').match(/([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
            },
            "downloaded": {
               "selector": ["td:contains('下载量') + td"],
               "filters": ["query.text().replace(/,/g,'').match(/([\\d.]+ ?[ZEPTGMK]?i?B)/)", "(query && query.length>=2)?(query[1]).sizeToNumber():0"]
            },
            "levelName": {
               "selector": ["td:contains('等级') + td"],
               "filters": ["query.find('b').text()"]
            },
              "bonus": {
               "selector": ["td:contains('奶糖') + td"],
               "filters": ["query.text().replace(/,/g,'')", "parseFloat(query)"]
            },
            "joinTime": {
                "selector": ["td:contains('加入日期') + td"],
                "filters": ["query.text().split(' (')[0]", "dateTime(query).isValid()?dateTime(query).valueOf():query"]
              }
          }
      },
      "userSeedingTorrents": {
         "prerequisites": "!user.seeding",
         "page": "/getusertorrentlistajax.php?userid=$user.id$&type=seeding",
         "fields": {
           "seeding": {
             "selector": ["b:first"],
             "filters": ["query.text()"]
           },
           "seedingSize": {
             "selector": "",
             "filters": [ "query.text().match(/总大小：(.*?)类型/g)",
                     "(query && query.length>0 ) ? query[0].replace('总大小：', '').replace('类型', '').trim() : 0",
                     "(query != 0) ? query.sizeToNumber() : 0"
                 ]
             }
         }
     }
   },
  "plugins": [
      {
    "name": "官方列表",
          "pages": [
              "/official.php"
          ],
          "scripts": [
              "/schemas/NexusPHP/common.js",
              "/schemas/NexusPHP/torrents.js"
